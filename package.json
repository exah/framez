{
  "name": "A",
  "version": "0.0.0",
  "description": "Collection of functions for creating web animations",
  "main": "lib/A.umd.js",
  "module": "lib/index.js",
  "repository": "exah/fun-animate",
  "author": "John Grishin <hi@johngrish.in>",
  "license": "MIT",
  "files": [ "lib" ],
  "scripts": {
    "watch": "rollup -c -w",
    "build": "npm run build:umd && npm run build:es6",
    "build:es6": "buble --objectAssign Object.assign -n modules -i src -o lib",
    "build:umd": "rollup -c",
    "prebuild": "rimraf lib",
    "prewatch": "rimraf lib",
    "lint": "eslint src"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  }
}
